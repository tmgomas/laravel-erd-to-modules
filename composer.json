{
    "name": "tmgomas/laravel-erd-to-modules",
    "description": "Generate Laravel modules structure from Mermaid ERD diagrams",
    "keywords": ["laravel", "erd", "mermaid", "module", "generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tmgomas/laravel-erd-to-modules"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "tmgomas\\ErdToModules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tmgomas\\ErdToModules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "tmgomas\\ErdToModules\\ErdToModulesServiceProvider"
            ],
            "aliases": {
                "ErdModules": "tmgomas\\ErdToModules\\Facades\\ErdModules"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}